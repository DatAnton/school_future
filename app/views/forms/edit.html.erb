<h1>Edit a form!</h1>
<p class="text-left">
  <%= link_to '<- Back', :back %>
</p>
<%= form_for(@form) do |f| %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="actions">
    <%= f.submit "Update", class: 'btn btn-primary', style: 'margin: 20px' %>
  </div>
<% end %>
