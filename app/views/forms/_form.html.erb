<li class="list-group-item justify-content-between align-items-center">
  <%= link_to form.name, form %>
  &nbsp;&nbsp;&nbsp;&nbsp;
  <span class="badge badge-primary badge-pill"><%= number_students(form) %></span>
  <%= link_to 'delete', form, class: "text-danger close", aria_label: "Close", method: :delete, data: { confirm: "You sure?" } if current_user.admin? %></span>
</li>
